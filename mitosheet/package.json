{
  "files": [
    "lib/**/*.js",
    "fonts/**/*",
    "dist/*.js",
    "css/**/*.css",
    "style/index.js"
  ],
  "jupyterlab": {
    "outputDir": "mitosheet/labextension",
    "extension": "lib/plugin"
  },
  "name": "mitosheet",
  "license": "AGPL-3.0-only",
  "author": {
    "name": "Mito Sheet",
    "email": "aaron@sagacollab.com"
  },
  "bugs": {
    "url": "https://github.com/mito-ds/monorepo/issues"
  },
  "repository": {
    "url": "https://github.com/mito-ds/monorepo",
    "type": "git"
  },
  "version": "0.2.1",
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/notebook": "^4.2.4",
    "@types/fscreen": "^1.0.1",
    "@types/react-dom": "^18.3.0",
    "fscreen": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "streamlit-component-lib": "^2.0.0"
  },
  "scripts": {
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf mitosheet/labextension",
    "clean:mito_frontend": "rimraf mitosheet/mito_frontend.js mitosheet/mito_frontend.css",
    "clean:dash": "cd mitosheet && rimraf mitodashwrapper.css && rimraf mitodashwrapper.js",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension && jlpm run clean:mito_frontend && jlpm run clean:dash",
    "clean": "jlpm run clean:all",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:mito_frontend:dev": "node esbuild.js",
    "build:streamlit:dev": "node esbuild.streamlit.js",
    "build:all:dev": "jlpm run build:labextension:dev && jlpm run build:mito_frontend:dev && jlpm run build:streamlit:dev",
    "build:dev": "jlpm run build:all:dev",
    "build:lib": "tsc --sourceMap",
    "build:labextension": "jupyter labextension build .",
    "build:mito_frontend": "node esbuild.js",
    "build:streamlit": "node esbuild.streamlit.js",
    "build:dash": "node esbuild.dash.js",
    "build:all": "jlpm run build:lib && jlpm run build:labextension && jlpm run build:mito_frontend && jlpm run build:streamlit && jlpm run build:dash",
    "build": "jlpm run build:all",
    "watch:lib": "tsc -w",
    "watch:labextension": "jupyter labextension watch .",
    "watch:mito_frontend": "node esbuild.js --watch",
    "watch:streamlit": "node esbuild.streamlit.js --watch",
    "watch:dash": "node esbuild.dash.js --watch",
    "watch:all": "jlpm run watch:lib & jlpm run watch:labextension & jlpm run watch:mito_frontend & jlpm run watch:streamlit & jlpm run watch:dash",
    "watch:jupyter": "run-p jlpm run watch:lib & jlpm run watch:labextension & jlpm run watch:mito_frontend",
    "watch:src": "npx tsc -w",
    "watch": "jlpm run watch:all",
    "lint:check": "eslint src/ --ext .ts,.tsx",
    "lint": "eslint src/ --ext .ts,.tsx --fix",
    "prepack": "jlpm run build:all",
    "build:docs:update_frontend": "python docs/make_function_docs.py update_frontend",
    "build:docs:generate_markdown": "python docs/make_function_docs.py generate_markdown"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@types/expect.js": "^0.3.29",
    "@types/node": "^18.19.43",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "esbuild": "^0.15.16",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-office-addins": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.3",
    "expect.js": "^0.3.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "mkdirp": "^1.0.4",
    "mocha": "^9.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^5.5.4"
  },
  "main": "lib/index.js",
  "homepage": "https://trymito.io",
  "types": "./lib/index.d.ts",
  "description": "The Mito Spreadsheet",
  "resolutions": {
    "@types/react": "^18.3.3"
  }
}
