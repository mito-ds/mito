    "DuckDB": {
        "schema": {
            "type": "object",
            "properties": {
                "driver": {
                    "type": "string",
                    "default": "duckdb"
                },
                "connectionName": {
                    "type": "string",
                    "title": "Source Name",
                    "default": "duckdb",
                    "pattern": "^\\w[\\w\\.]*$"
                },
                "database": {
                    "type": "string",
                    "title": "Path to database",
                    "default": ":memory:"
                }
            },
            "required": [
                "connectionName",
                "database",
                "driver"
            ],
            "additionalProperties": false
        }
    },
